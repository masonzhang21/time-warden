{
  "name": "Time Warden",
  "version": "1.0.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": { 
      "16": "logo16.png",
      "48": "logo48.png",
     "128": "logo128.png" }
  },

  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "storage", "tabs", "webNavigation", "activeTab"
  ],
  "icons": { 
    "16": "logo16.png",
    "48": "logo48.png",
   "128": "logo128.png" },
   "web_accessible_resources": [
    "content-script.js"
]
   
}
